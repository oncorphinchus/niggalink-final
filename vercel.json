{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/login",
        "dest": "/login.html"
      },
      {
        "src": "/",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }